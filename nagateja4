#include<stdio.h>
int input(int a[],int n);
int sum(int a[]);
void output(int s);
int main()
{
int i,n,a[100],s=0;
input(a,n);
s=sum(a);
output(s);
}
int input(int a[],int n)
{
int i;
printf("enter the value of n\n");
scanf("%d",&n);
printf("enter %d element\n",n);
for(i=0;i<n;i++)
{
scanf("%d",&a);
}
}
int sum(int a[])
{
int s,n,i;
for(i=0;i<n;i++)
s=s+a[i];
return s;
}
void output(int s)
{
printf("enter sum=%d",s);
}






